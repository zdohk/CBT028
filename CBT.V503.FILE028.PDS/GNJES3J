    SET &OUT = &STR(//Z54#JGBH JOB (57700),G.B.HAMPTON,NOTIFY=Z54,TIME=-
60,   )
    PUTFILE OUT
    SET &OUT = &STR(//    REGION=4096K,MSGCLASS=U                      -
    )
    PUTFILE OUT
    SET &OUT = &STR(//*MAIN  CLASS=SYSGEN,ORG=LOCAL                    -
    )
    PUTFILE OUT
    SET &OUT = &STR(//*FORMAT PR,DDNAME=,DEST=LOCAL,PRTY=252           -
    )
    PUTFILE OUT
